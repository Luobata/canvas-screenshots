global_prefix = '#screen-shoot'

{global_prefix}
    position absolute
    display none
    .function-box
        height 40px
        line-height 40px
        border-radius 5px
        background gray
        padding 0 10px
        .box-item
            display inline-block
            width 20px
            height 20px
            border-radius 5px
            line-height 20px
            margin 0 10px
            vertical-align middle
            background red
            color white
            cursor pointer
            text-align center